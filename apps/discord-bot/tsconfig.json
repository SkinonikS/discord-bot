{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./dist",
    "paths": {
      "#root": [
        "./"
      ],
      "#root/*": [
        "./*"
      ],
      "#app": [
        "./app"
      ],
      "#app/*": [
        "./app/*"
      ],
      "#locales": [
        "./locales"
      ],
      "#locales/*": [
        "./locales/*"
      ],
      "#start": [
        "./start"
      ],
      "#start/*": [
        "./start/*"
      ],
      "#config": [
        "./config"
      ],
      "#config/*": [
        "./config/*"
      ],
      "#bootstrap": [
        "./bootstrap"
      ],
      "#bootstrap/*": [
        "./bootstrap/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}
