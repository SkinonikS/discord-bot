      "import": "./dist/src/bootstrappers/*.js",
      "node": "./dist/src/bootstrappers/*.js",
      "types": "./dist/src/bootstrappers/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@adonisjs/fold": "^10.2.0",
    "@module/logger": "workspace:*",
    "@poppinss/exception": "^1.2.1",
    "@poppinss/hooks": "^7.2.5",
    "@poppinss/utils": "^6.9.4",
    "dot-prop": "^9.0.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0"
  },
  "packageManager": "pnpm@10.6.3"
}
