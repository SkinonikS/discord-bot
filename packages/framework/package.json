{
  "private": true,
  "name": "@package/framework",
  "type": "module",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.6.3",
  "devDependencies": {
    "@package/module-logger": "workspace:*"
  },
  "dependencies": {
    "@poppinss/hooks": "^7.2.5",
    "dot-prop": "^9.0.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bootstrappers/*": {
      "import": "./dist/src/bootstrappers/*.js",
      "node": "./dist/src/bootstrappers/*.js",
      "types": "./dist/src/bootstrappers/*.d.ts"
    },
    "./package.json": "./package.json"
  }
}
