    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "eslint-plugin-import-x": "^4.15.0",
    "tsdown": "^0.12.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^13.3.0",
    "@adonisjs/fold": "^10.1.3",
    "@poppinss/hooks": "^7.2.5",
    "dot-prop": "^9.0.0",
